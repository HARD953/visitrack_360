{
  "expo": {
    "name": "Visitrack_360",
    "slug": "visitrackapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/myicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ]
      ],
      "permissions": {
        "camera": true,
        "mediaLibrary": true
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.samajeste01.Visitrack_360"
    },
    "web": {
      "favicon": "./assets/Images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "37a808d9-6773-4fc5-8fca-835a4d6bceff"
      }
    },
    "plugins": [
      "expo-build-properties"
    ],
    "owner": "samajeste01"
  }
}
